{
  "name": "@downtown65/frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "ci:lint": "biome ci",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "pnpm run generate:all && react-router dev",
    "format": "biome format",
    "lint": "biome check",
    "postinstall": "pnpm run generate:react-router-types && pnpm run generate:cloudflare-types",
    "preview": "pnpm run build && vite preview",
    "test": "vitest",
    "typecheck": "tsc -b --noEmit",
    "generate:all": "pnpm run generate:react-router-types && pnpm run generate:cloudflare-types && pnpm run generate:api-types",
    "generate:api-types": "openapi-typescript http://localhost:3002/doc -o ./.generated/types/app/api/+types/api.d.ts",
    "generate:react-router-types": "react-router typegen",
    "generate:cloudflare-types": "wrangler types",
    "knip": "knip"
  },
  "dependencies": {
    "@downtown65/logger": "workspace:*",
    "@downtown65/schema": "workspace:*",
    "@mantine/core": "8.3.10",
    "@mantine/dates": "8.3.10",
    "@mantine/form": "8.3.10",
    "@mantine/hooks": "8.3.10",
    "@mantine/tiptap": "8.3.10",
    "@react-router/fs-routes": "7.9.6",
    "@tabler/icons-react": "3.35.0",
    "@tiptap/extension-link": "3.13.0",
    "@tiptap/pm": "3.13.0",
    "@tiptap/react": "3.13.0",
    "@tiptap/starter-kit": "3.13.0",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "dayjs": "1.11.19",
    "isbot": "5.1.32",
    "jwt-decode": "4.0.0",
    "openapi-fetch": "0.15.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-router": "7.10.1",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@cloudflare/vite-plugin": "1.17.1",
    "@react-router/dev": "7.10.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "16.3.0",
    "@types/node": "22.13.14",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.0.6",
    "jsdom": "27.2.0",
    "knip": "5.71.0",
    "openapi-typescript": "7.10.1",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "5.9.3",
    "vite": "7.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "4.0.15",
    "wrangler": "4.54.0"
  }
}
