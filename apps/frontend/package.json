{
  "name": "@downtown65/frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "ci:lint": "biome ci",
    "deploy": "pnpm run build && wrangler deploy",
    "dev": "pnpm run generate:local && react-router dev",
    "format": "biome format",
    "lint": "biome check",
    "postinstall": "pnpm run generate:react-router-types && pnpm run generate:cf",
    "preview": "pnpm run build && vite preview",
    "test": "vitest",
    "typecheck": "tsc -b --noEmit",
    "generate:local": "pnpm run generate:react-router-types && pnpm run generate:api-types:server && npx wrangler types --config ./wrangler.local.jsonc",
    "generate:api-types:server": "openapi-typescript http://localhost:3002/doc -o ./.generated/types/app/api/+types/api.d.ts",
    "generate:api-types:file": "openapi-typescript ../backend/.generated/openapi.json  -o ./.generated/types/app/api/+types/api.d.ts",
    "generate:react-router-types": "react-router typegen",
    "generate:cf": "wrangler types",
    "ci:generate": "pnpm run generate:react-router-types && pnpm run generate:cf && pnpm run generate:api-types:file",
    "knip": "knip",
    "clean": "rimraf node_modules"
  },
  "dependencies": {
    "@downtown65/logger": "workspace:*",
    "@downtown65/schema": "workspace:*",
    "@mantine/core": "8.3.10",
    "@mantine/dates": "8.3.10",
    "@mantine/form": "8.3.10",
    "@mantine/hooks": "8.3.10",
    "@mantine/tiptap": "8.3.10",
    "@react-router/fs-routes": "7.11.0",
    "@tabler/icons-react": "3.36.1",
    "@tiptap/extension-link": "3.14.0",
    "@tiptap/pm": "3.14.0",
    "@tiptap/react": "3.14.0",
    "@tiptap/starter-kit": "3.14.0",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "dayjs": "1.11.19",
    "isbot": "5.1.32",
    "jwt-decode": "4.0.0",
    "openapi-fetch": "0.15.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router": "7.11.0",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@cloudflare/vite-plugin": "1.20.1",
    "@react-router/dev": "7.11.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "jsdom": "27.4.0",
    "knip": "5.79.0",
    "openapi-typescript": "7.10.1",
    "postcss": "8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "postcss-simple-vars": "7.0.1",
    "rimraf": "6.0.1",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "3.2.0",
    "wrangler": "4.58.0"
  }
}
