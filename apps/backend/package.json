{
  "name": "backend",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --port 3002",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "test": "vitest",
    "lint": "biome check",
    "ci:lint": "biome ci",
    "postinstall": "pnpm run cf-typegen",
    "format": "biome format",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "cf-typegen": "wrangler types",
    "deploy": "wrangler deploy"
  },
  "dependencies": {
    "@downtown65/schema": "workspace:*",
    "@hono/node-server": "1.19.6",
    "@hono/zod-openapi": "1.1.5",
    "@scalar/hono-api-reference": " 0.9.25",
    "auth0": "5.1.0",
    "hono": "4.10.7",
    "jwt-decode": "4.0.0",
    "nanoid": "5.1.6",
    "openapi3-ts": "4.5.0",
    "ulidx": "^2.4.1",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/vitest-pool-workers": "0.10.13",
    "@types/node": "22.13.14",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "5.9.3",
    "vitest": "^2.1.3",
    "wrangler": "4.51.0"
  }
