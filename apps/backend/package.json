{
  "name": "@downtown65/backend",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --port 3002",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "test": "vitest",
    "lint": "biome check",
    "ci:lint": "biome ci",
    "postinstall": "pnpm run cf-typegen",
    "format": "biome format",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "cf-typegen": "wrangler types",
    "deploy": "wrangler deploy",
    "find-unused": "knip"
  },
  "dependencies": {
    "@downtown65/logger": "workspace:*",
    "@downtown65/schema": "workspace:*",
    "@hono/zod-openapi": "1.1.5",
    "@scalar/hono-api-reference": " 0.9.25",
    "auth0": "5.1.0",
    "drizzle-orm": "0.45.0",
    "hono": "4.10.7",
    "jwt-decode": "4.0.0",
    "nanoid": "5.1.6",
    "openapi3-ts": "4.5.0",
    "ulidx": "^2.4.1",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@cloudflare/vitest-pool-workers": "0.10.14",
    "@types/node": "22.13.14",
    "@vitest/coverage-v8": "4.0.15",
    "dotenv": "17.2.3",
    "drizzle-kit": "0.31.8",
    "knip": "5.71.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vitest": "4.0.15",
    "wrangler": "4.51.0"
  }
}
