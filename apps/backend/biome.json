{
  "$schema": "https://biomejs.dev/schemas/1.9.1/schema.json",
  "extends": ["../../biome.json"],
  "files": {
    "include": ["src/**/*.ts", "vitest.config.ts"]
  },
  "linter": {
    "rules": {
      "recommended": true,
      "suspicious": {
        "noConsole": {
          "level": "error",
          "options": {
            "allow": ["error", "warn"]
          }
        }
      }
    }
  }
}
